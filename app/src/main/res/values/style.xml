<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="BottomNavigationViewSelectedTextStyle" parent="TextAppearance.AppCompat.Medium">
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#ffffff</item>
    </style>

    <style name="BottomNavigationViewUnselectedTextStyle" parent="TextAppearance.AppCompat.Medium">
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">#dedede</item>
    </style>
    <style name="circleImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">10%</item>
    </style>
    <style name="circleImageViewTopCorners" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopLeft">10dp</item>
        <item name="cornerSizeTopRight">10dp</item>
        <item name="cornerSizeBottomLeft">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
    </style>
    <style name="circleImageViewStartCorners" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopLeft">16dp</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerSizeBottomLeft">10dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
    </style>
</resources>